pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    tools {
        maven 'M3'
        jdk 'open jdk 11'
    }
    stages {
        stage('Build jar') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
			post {
				always {
					archiveArtifacts artifacts: 'target/**/*.jar', fingerprint: true
		    }
          }
        }
     /*   stage('Test') {
            steps {
                sh 'mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        } */

		
    }
}